<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>obs</Title>
			<Shortcut>obs</Shortcut>
			<Description>Code snippet for 'Observable&lt;T&gt;'</Description>
			<Author>David Piepgrass</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>type</ID>
					<Default>string</Default>
					<ToolTip>Data type</ToolTip>
				</Literal>
				<Literal>
					<ID>name</ID>
					<Default>Name</Default>
					<ToolTip>Property Name</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[private Observable<$type$> _$name$ = new Observable<$type$>(default($type$));
		public $type$ $name$
		{
			get { return _$name$; }
			set { _$name$.Value = value; }
		}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>